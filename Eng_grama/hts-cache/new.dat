    <td width="82" height="23" valign="top"></td>
    <td width="35" height="60" rowspan="2" valign="top"><img src="../images/logo_lesson.jpg" width="35" height="44"></td>
    <td width="2" height="23" valign="top"></td>
    <td width="384" height="23" valign="top"></td>
